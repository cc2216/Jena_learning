@prefix fa: <http://www.semanticweb.org/ontologies/2010/0/family.owl#>.@include <RDFS>.@include <OWL>.  
[rule1:(?a fa:hasFather ?b) (?c fa:hasFather ?d) (?b fa:hasBrother ?d) notEqual(?b,?d)->(?a fa:hasCousin ?b)]  
[rule2:(?a fa:hasFather ?b)(?c fa:hasFather ?d)(?b fa:hasFather ?e)(?d fa:hasFather ?e) notEqual(?b,?d)->(?a fa:hasCousin ?c)]  
[rule3:(?a fa:hasSon ?b)(?b fa:hasSon ?c)->(?a fa:hasGrandSon ?c)]  
[rule4:(?a fa:hasSon ?b)(?b fa:hasDaughter ?c)->(?a fa:hasGrandDaughter ?c)]  
[rule5:(?a fa:hasFather ?b)(?b fa:hasBrother ?c)->(?a fa:hasUncle ?c)]  
[rule6:(?a fa:hasFather ?b)(?b fa:hasSister ?c)->(?a fa:hasAunt ?c)]  
[rule7:(?a fa:hasGrandSon ?b)->(?b fa:isGrandSonOf ?a)]  
[rule8:(?a fa:hasAunt ?b)->(?b fa:isAuntOf ?a)]  
[rule9:(?a fa:hasFather ?b)->(?b fa:isFatherOf ?a)]  
[rule10:(?a fa:hasMother ?b)->(?b fa:isMotherOf ?a)]  
[rule11:(?a fa:hasBrother ?b)->(?b fa:isBrotherOf ?a)]  
[rule12:(?a fa:hasSister ?b)->(?b fa:isSisterOf ?b)]  
[rule13:(?a fa:hasCousin ?b)->(?b fa:isCousinOf ?a)]  
[rule14:(?a fa:hasSon ?b)->(?b fa:isSonOf ?a)]  
[rule15:(?a fa:hasDaughter ?b)->(?b fa:isDaughterOf ?a)]  
[rule16:(?a fa:hasGrandDaughter ?b)->(?b fa:isGrandDaughterOf ?a)]  
[rule17:(?a fa:hasWife ?b)->(?b fa:isWifeOf ?a)]  
[rule18:(?a fa:hasHusband ?b)->(?b fa:isHusbandOf ?a)]  
[rule19:(?a fa:hasUncle ?b)->(?b fa:isUncleOf ?a)] 